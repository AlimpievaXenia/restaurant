<div class="content-container">

<div class="container">
  <div class="coldSnacks">
    {{#each coldSnacks}}
      <div data-mealid="{{this.id}}" class="js-meal">
        <img style="height: 200px; width: 200px" src="{{'Photos.url'}}" alt=""><br>
          <ul class="meal-ul">
              <li>{{this.title}}</li>
              <li>{{this.weight}}</li>
              <li>{{this.ingredients}}</li>
              <li class="js-meal-price">{{this.price}}</li>
              <li><button id="basketBtn" class="btnClass">В корзину</button></li>
          </ul>
      </div>
    {{/each}}
